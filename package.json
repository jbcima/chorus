{
  "name": "chorus",
  "version": "1.0.0",
  "private": true,
  "description": "chorus",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "rimraf .cache && rimraf public && gatsby build",
    "deploy": "cd \"dist-remote/chorus\" && git pull && git checkout -b main && cd \"../../\" && rimraf --glob \"dist-remote/chorus/*\" && cpx \"public/**/*.*\" \"dist-remote/chorus\" && cd dist-remote/chorus && git add . && git commit -m \"Update\" && git push -u origin main",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "cpx2": "^7.0.1",
    "decap-cms-app": "^3.1.1",
    "gatsby": "^5.13.2",
    "gatsby-adapter-netlify": "^1.1.3",
    "gatsby-plugin-decap-cms": "^4.0.4",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "use-sound": "^4.0.1"
  }
}