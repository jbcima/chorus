{
  "name": "chorus",
  "version": "1.0.0",
  "private": true,
  "description": "chorus",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "rimraf .cache && rimraf public && gatsby build",
    "deploy": "rimraf dist-remote/chorus/* && cpx \"public/**/*.*\" dist-remote/chorus && cd dist-remote/chorus && git add . && git commit -m \"Update\" && mkdir -p ~/.ssh && echo -e \"$(SSH_KEY//_/\\n)\" > ~/.ssh/id_rsa && chmod og-rwx ~/.ssh/id_rsa && GIT_SSH_COMMAND=\"ssh -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no\" && git push -u origin main",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "cpx": "^1.5.0",
    "decap-cms-app": "^3.1.1",
    "gatsby": "^5.13.2",
    "gatsby-adapter-netlify": "^1.1.3",
    "gatsby-plugin-decap-cms": "^4.0.4",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "use-sound": "^4.0.1"
  }
}
